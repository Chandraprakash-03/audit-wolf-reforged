{
  "name": "audit-wolf-backend",
  "version": "1.0.0",
  "description": "Backend API for Audit Wolf smart contract auditing platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "migrate": "ts-node src/migrations/migrate.ts",
    "test": "jest",
    "test:auth": "jest src/test/auth.test.ts",
    "test:db": "ts-node src/test/database.test.ts",
    "test:slither": "jest src/test/slither.test.ts",
    "test:analysis": "jest src/test/analysis.test.ts",
    "test:ai": "jest src/test/ai-analysis.test.ts",
    "test:ai-integration": "jest src/test/ai-integration.test.ts",
    "test:reports": "jest src/test/report-generation.test.ts",
    "test:report-api": "jest src/test/report-api.test.ts",
    "test:email": "jest src/test/email.test.ts",
    "test:watch": "jest --watch",
    "test:slither-integration": "node scripts/test-slither.js",
    "test:ai-integration-demo": "node scripts/test-ai-analysis.js",
    "test:report-generation-demo": "node scripts/test-report-generation.js",
    "test:email-demo": "node scripts/test-email.js",
    "test:notifications": "jest src/test/notifications.test.ts",
    "test:notifications-demo": "node scripts/test-notifications.js",
    "test:ipfs-blockchain": "jest src/test/ipfs-blockchain.test.ts",
    "test:decentralized-storage-demo": "node scripts/test-decentralized-storage.js",
    "test:integration": "jest src/test/integration --runInBand",
    "test:load": "artillery run src/test/load/artillery-config.yml",
    "test:load-report": "artillery run src/test/load/artillery-config.yml --output load-results.json && artillery report load-results.json",
    "seed:test-data": "ts-node src/test/fixtures/seed-database.ts",
    "docs:serve": "node -e \"console.log('API Documentation: http://localhost:3001/api-docs')\"",
    "docs:generate": "swagger-jsdoc -d src/docs/openapi.yaml src/routes/*.ts -o api-docs.json",
    "test:all": "node scripts/run-all-tests.js",
    "test:all-unit": "node scripts/run-all-tests.js --unit-only",
    "test:all-integration": "node scripts/run-all-tests.js --integration-only",
    "test:all-load": "node scripts/run-all-tests.js --load-only",
    "test:multi-blockchain": "ts-node src/test/run-multi-blockchain-tests.ts",
    "test:multi-blockchain-unit": "jest src/test/multi-blockchain-comprehensive.test.ts --verbose",
    "test:multi-blockchain-integration": "jest src/test/integration/multi-blockchain-integration.test.ts --verbose --testTimeout=600000",
    "test:multi-blockchain-performance": "jest src/test/performance/multi-blockchain-performance.test.ts --verbose --testTimeout=900000",
    "test:solana": "jest src/test/solana-analyzer.test.ts --verbose",
    "test:cardano": "jest src/test/cardano-analyzer.test.ts --verbose",
    "test:cross-chain": "jest src/test/cross-chain-analyzer.test.ts --verbose",
    "test:validate-setup": "ts-node src/test/validate-test-setup.ts"
  },
  "keywords": [
    "smart-contracts",
    "auditing",
    "security",
    "blockchain"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@supabase/supabase-js": "^2.56.0",
    "@types/html-pdf-node": "^1.0.2",
    "bull": "^4.12.9",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.1",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.2.0",
    "helmet": "^8.1.0",
    "html-pdf-node": "^1.0.8",
    "ioredis": "^5.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.31",
    "morgan": "^1.10.1",
    "puppeteer": "^24.17.1",
    "senderwolf": "^2.0.4",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tmp": "^0.2.3",
    "typescript": "^5.9.2",
    "yamljs": "^0.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@sentry/nextjs": "^10.8.0",
    "@sentry/node": "^10.8.0",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/tmp": "^0.2.6",
    "@types/yamljs": "^0.2.34",
    "artillery": "^2.0.24",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}